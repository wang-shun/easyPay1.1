<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    style="@style/layout1">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/mendianmingcheng"/>


                    <TextView
                        android:id="@+id/mendianmingcheng_tv"
                        style="@style/gaojishezhi_rigth_text"
                        android:text="mendianmingcheng_tv"
                        />


                    <TextView
                        android:id="@+id/click_to_manager"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="@dimen/dp15"
                        android:textColor="@color/color_blue"
                        android:textSize="@dimen/sp17"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/mendianid"
                        />


                    <TextView
                        android:id="@+id/mendianid_tv"
                        android:text="mendianid_tv"
                        style="@style/gaojishezhi_rigth_text"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/suoshupinpai"
                        />


                    <TextView
                        android:id="@+id/suoshupinpai_tv"
                        android:text="suoshupinpai_tv"
                        style="@style/gaojishezhi_rigth_text"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        android:text="@string/pinpaiid"
                        style="@style/gaojishezhi_left_text"/>


                    <TextView
                        android:id="@+id/pinpaiid_tv"

                        android:text="pinpaiid_tv"
                        style="@style/gaojishezhi_rigth_text"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/suoshuquyu"
                   />


                    <TextView
                        android:id="@+id/suoshuquyu_tv"

                        android:text="suoshuquyu_tv"
                        style="@style/gaojishezhi_rigth_text"/>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/mendianleixing"
                       />


                    <TextView
                        android:id="@+id/mendianleixing_tv"

                        android:text="mendianleixing_tv"
                        style="@style/gaojishezhi_rigth_text"/>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/mendiandizhi"
                 />


                    <TextView
                        android:id="@+id/mendiandizhi_tv"

                        android:text="mendiandizhi_tv"
                        style="@style/gaojishezhi_rigth_text"/>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/mendianzuoji"
                       />


                    <TextView
                        android:id="@+id/mendianzuoji_tv"
                        style="@style/gaojishezhi_rigth_text"
                        android:text="mendianzuoji_tv"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView

                        android:text="@string/yingyemianji"
                        style="@style/gaojishezhi_left_text"/>


                    <TextView
                        android:id="@+id/yingyemianji_tv"
                        style="@style/gaojishezhi_rigth_text"
                        android:text="yingyemianji_tv"/>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        style="@style/gaojishezhi_left_text"
                        android:text="@string/guanliyuan"
                       />


                    <TextView
                        android:id="@+id/guanliyuan_tv"
                        style="@style/gaojishezhi_rigth_text"
                        android:text="guanliyuan_tv"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <TextView
                        android:textColor="@color/color_red"
                        android:text="@string/shouquandaoqi"
                        style="@style/gaojishezhi_left_text"/>


                    <TextView
                        android:id="@+id/shouquandaoqi_tv"
                        style="@style/gaojishezhi_rigth_text"
                        android:text="shouquandaoqi_tv"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp10"
                    android:background="@color/color_666666"
                    android:orientation="horizontal"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="9"
                        android:width="0dp"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp10"
                        android:text="@string/dingdancaozuo"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/sp25"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dp10"
                            android:text="@string/dingdancaozuo1"
                            android:textColor="@color/gray"
                            android:textSize="@dimen/sp20"/>

                        <Switch
                            android:id="@+id/jiaobanprint_switch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dp10"/>
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp10"
                        android:layout_marginLeft="@dimen/dp20"
                        android:layout_marginRight="@dimen/dp20"
                        android:background="@drawable/horizontal_dash_line"
                        android:layerType="software"/>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dp10"
                            android:text="@string/dingdancaozuo2"
                            android:textColor="@color/gray"
                            android:textSize="@dimen/sp20"/>

                        <Switch
                            android:id="@+id/refundprint_switch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dp10"/>
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp10"
                        android:layout_marginLeft="@dimen/dp20"
                        android:layout_marginRight="@dimen/dp20"
                        android:background="@drawable/horizontal_dash_line"
                        android:layerType="software"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp10"
                    android:background="@color/color_666666"
                    android:orientation="horizontal"
                    >


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="9"
                        android:width="0dp"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp10"
                        android:text="@string/quanxiananquan"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/sp25"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dp10"
                            android:text="@string/quanxiananquan1"
                            android:textColor="@color/gray"
                            android:textSize="@dimen/sp20"/>

                        <Switch
                            android:id="@+id/quanxian_switch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/dp10"/>
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp10"
                        android:layout_marginLeft="@dimen/dp20"
                        android:layout_marginRight="@dimen/dp20"
                        android:background="@drawable/horizontal_dash_line"
                        android:layerType="software"/>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

    </ScrollView>
</LinearLayout>